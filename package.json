		"builtin-modules": "3.3.0",
		"esbuild": "0.17.3",
		"eslint-config-prettier": "^9.0.0",
		"obsidian": "latest",
		"prettier": "^3.0.2",
		"tslib": "2.4.0",
		"typescript": "4.7.4"
	},
	"dependencies": {
		"@popperjs/core": "^2.11.8",
		"gray-matter": "^4.0.3",
		"rm": "^0.1.8"
